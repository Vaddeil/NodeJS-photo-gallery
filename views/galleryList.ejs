<%- include('partials/header') %>
<%- include('partials/nav') %>

<main>
  <% console.log(gallery); for (photo of gallery) { %>
  <a href="/gallery/<%= photo.id %>">
    <figure>
      <img src="/images/sm/<%= photo.fileName %>" alt="<%= photo.title %>" />
      <figcaption>
        <h1><%= photo.attribution.credit %></h1>
        <a href="<%= photo.attribution.url %>"></a>
      </figcaption>
    </figure>
  </a>
  <% } %>
</main>
<%- include('partials/footer') %>
