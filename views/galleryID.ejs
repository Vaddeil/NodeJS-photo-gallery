<%- include('partials/header') %> 
<%- include('partials/nav') %>

<main>
  <% for (photo of gallery) { if(photo.id == photoid){ %>
  <a href="/gallery/<%= photo.id %>">
    <figure>
      <img src="../images/lrg/<%= photo.fileName %>" alt="<%= photo.title %>" />
      <figcaption>
        <h1><%= photo.attribution.credit %></h1>
        <h2>Source Website: Lorem Picsum</h2>
        <a href="<%= photo.attribution.url %>"></a>
      </figcaption>
    </figure>
  </a>
  <% }} %>
</main>

<%- include('partials/footer') %>
